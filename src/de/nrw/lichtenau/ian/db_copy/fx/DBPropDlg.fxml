<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane fx:id="root" prefHeight="145.0" prefWidth="288.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.nrw.lichtenau.ian.db_copy.fx.DBPropDlgController">
   <center>
      <GridPane BorderPane.alignment="CENTER">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="135.0" minWidth="10.0" prefWidth="79.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="214.0" minWidth="10.0" prefWidth="209.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Name" GridPane.rowIndex="1" />
            <Label text="URL" GridPane.rowIndex="2" />
            <Label text="User" GridPane.rowIndex="3" />
            <Label text="Password" GridPane.rowIndex="4" />
            <TextField fx:id="txtFldName" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <TextField fx:id="txtFldURL" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <TextField fx:id="txtFldUser" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <TextField fx:id="txtFldPassword" GridPane.columnIndex="1" GridPane.rowIndex="4" />
            <Label text="Driver" />
            <TextField fx:id="txtFldDriver" GridPane.columnIndex="1" />
         </children>
      </GridPane>
   </center>
   <bottom>
      <HBox alignment="CENTER_RIGHT" prefHeight="20.0" prefWidth="200.0" spacing="10.0" BorderPane.alignment="CENTER">
         <children>
            <Button mnemonicParsing="false" onAction="#onActionOk" text="Ok" />
            <Button mnemonicParsing="false" onAction="#onActionCancel" text="Cancel" />
         </children>
      </HBox>
   </bottom>
</BorderPane>
